{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "jsx": "react-native",
      "baseUrl": "./app",
      "paths": {
            "@api": ["api"],
            "@assets": ["assets"],
            "@components": ["components"],
            "@configs": ["configs"],
            "@models": ["models"],
            "@mini": ["mini"],
            "@navigation": ["navigation"],
            "@screens": ["screens"],
            "@navigator": ["navigation/navigator"],
            "@redux": ["redux"],
            "@store": ["redux/store"],
            "@actions": ["redux/actions"],
            "@selectors": ["redux/selectors"],
            "@services": ["services"],
            "@utils": ["utils"],
      }
    },
    "include": ["app"],
    "exclude": ["node_modules"]
  }